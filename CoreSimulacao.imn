node n1 {
    type router
    model host
    network-config {
	hostname FSTracker
	!
	interface eth0
	 ip address 10.0.0.10/24
	 ipv6 address 2001:0::10/64
	!
    }
    canvas c1
    iconcoords {167.0 100.0}
    labelcoords {167.0 132.0}
    interface-peer {eth0 n6}
}

