;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	cc.pt. root.cc.pt. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; name servers - NS records
@	IN	NS	ns1.cc.pt.
@	IN	NS	ns2.cc.pt.

; name servers - A records
ns1.cc.pt.	IN	A	10.0.20.10
ns2.cc.pt.	IN	A	10.0.24.10

; 
node1.cc.pt.	IN	A	10.0.10.20
node2.cc.pt.	IN	A	10.0.11.20
