zone "cc.com"{
	type primary;
	file "/etc/bind/com/zones/db.cc.com";
	allow-transfer{10.0.17.10;};
};

zone "0.10.in-addr.arpa"{
	type primary;
	file "/etc/bind/com/zones/db.0.10";
	allow-transfer{10.0.17.10;};
};
