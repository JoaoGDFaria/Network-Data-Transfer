;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	cc.pt. root.cc.pt. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; name servers
	IN	NS	ns1.cc.pt.
	IN	NS	ns2.cc.pt.

; PTR records
20.1	IN	PTR	node1.cc.pt.
20.2	IN	PTR	node2.cc.pt.
